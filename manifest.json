{
  "manifest_version": 3,
  "name": "Gravity Scroll",
  "version": "1.0",
  "description": "Scrolls to bottom with gravitational acceleration",
  "permissions": ["activeTab"],
  "action": {
    "default_title": "Gravity Scroll"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}
